input {
  redis {
    host => "127.0.0.1"
    type => "redis-input"
    data_type => "list"
    key => "logstash"
  }
  syslog {
    type => syslog
    port => 5514
  }
}

output {
{% if logstash_output_stdout %}
  stdout {  }
{% endif %}
{% if logstash_output_logentries %}
  logentries {
    token => "{{ logstash_logentries_token }}"
  }
{% endif %}
}
